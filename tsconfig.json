{
	"compileOnSave" : false,
	"compilerOptions" : {
		"strict" : true,
		"outDir" : "./dist/out-tsc",
		"module" : "ESNext",
		"sourceMap" : true,
		"declaration" : false,
		"moduleResolution" : "node",
		"emitDecoratorMetadata" : true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators" : true,
		"allowJs" : true,
		"target" : "ES2022",
		"types" : [
			"node"
		],
		"lib" : [
			"es2017",
			"es2016",
			"es2015",
			"es2018",
			"dom"
		],
		"useDefineForClassFields" : false
	}
}
